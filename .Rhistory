data <- read.csv("train.csv")
library(ggplot2)
View(data)
ggplot(data = data, aes(x = Pclass, fill = Survived)) + geom_histogram()
ggplot(data = data, aes(x = Pclass, color = Survived)) + geom_histogram()
ggplot(data = data, aes(x = Pclass, color = Survived)) + geom_histogram(col = Survived)
ggplot(data = data, aes(x = Pclass, col = Survived)) + geom_histogram()
ggplot(data = data, aes(x = Pclass)) + geom_histogram()
ggplot(data = data, aes(x = Pclass, color = Survived)) + geom_histogram()
str(data)
data$Survived = as.factor(data$Survived)
ggplot(data = data, aes(x = Pclass, color = Survived)) + geom_histogram()
ggplot(data = data, aes(x = Sex, color = Survived)) + geom_histogram()
ggplot(data = data, aes(x = Sex, color = Survived), stat="count") + geom_histogram()
ggplot(data = data, aes(x = Sex, color = Survived)) + geom_histogram(stat = "count")
ggplot(data = data, aes(x = Age, color = Survived)) + geom_histogram()
ggplot(data = data, aes(x = Sibsp, color = Survived)) + geom_histogram()
ggplot(data = data, aes(x = SibSp, color = Survived)) + geom_histogram()
ggplot(data = data, aes(x = Parch, color = Survived)) + geom_histogram()
ggplot(data = data, aes(x = Fare, color = Survived)) + geom_histogram()
ggplot(data = data, aes(x = Embarked, color = Survived)) + geom_histogram()
ggplot(data = data, aes(x = Embarked, color = Survived)) + geom_histogram(stat="count")
library(randomForest)
install.packages("randomForest")
library(randomForest)
fit <- randomForest(Survived ~ Pclass + Sex + Age, data = data)
data$Age[is.na(data$Age)] <- 0
fit <- randomForest(Survived ~ Pclass + Sex + Age, data = data)
print(fit)
importance(fit)
testData <- read.csv("test.csv")
View(testData)
?predict
predict(fit, newdata = testData)
testData$Age[is.na(testData$Age)] <- 0
predict(fit, newdata = testData)
predicted <- predict(fit, newdata = testData)
write.csv(predicted)
testData$Predicted <- predicted
write.csv(testData[c('PassengerId', 'Predicted')])
?write.csv
write.csv(testData[c('PassengerId', 'Predicted')], file="predicted.csv")
